generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model equipos{
  idEquipo Int @id @default(autoincrement())
  nombreEquipo String @db.VarChar(255)
  institucion String
  departamento String
  municipio String
  direccion String
  telefono String
}